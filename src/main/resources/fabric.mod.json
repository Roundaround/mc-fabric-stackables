{
  "schemaVersion": 1,
  "id": "stackables",
  "version": "${version}",
  "name": "Stackables",
  "description": "Make some non-stackable items stackable.\n${library}",
  "authors": [
    "Roundaround"
  ],
  "contact": {
    "homepage": "https://modrinth.com/mod/stackables",
    "sources": "https://github.com/Roundaround/mc-fabric-stackables",
    "issues": "https://github.com/Roundaround/mc-fabric-stackables/issues"
  },
  "license": "MIT",
  "icon": "assets/stackables/icon.png",
  "environment": "*",
  "entrypoints": {
    "fabric-datagen": [
      "me.roundaround.stackables.data.StackablesDataGenerator"
    ],
    "main": [
      "me.roundaround.stackables.StackablesMod"
    ],
    "modmenu": [
      "me.roundaround.stackables.compat.modmenu.ModMenuImpl"
    ]
  },
  "mixins": [
    "stackables.mixins.json"
  ],
  "accessWidener": "stackables.accesswidener",
  "depends": {
    "fabricloader": ">=0.13.3",
    "fabric-api-base": "*",
    "minecraft": "1.20.*",
    "java": ">=17"
  }
}
