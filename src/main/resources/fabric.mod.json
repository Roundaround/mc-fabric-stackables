  "authors": [
    "Roundaround"
  ],
  "contact": {
    "homepage": "https://modrinth.com/mod/roundaround-stackables",
    "sources": "https://github.com/Roundaround/mc-fabric-stackables",
    "issues": "https://github.com/Roundaround/mc-fabric-stackables/issues"
  },
  "license": "MIT",
  "icon": "assets/stackables/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "me.roundaround.stackables.StackablesMod"
    ],
    "modmenu": [
      "me.roundaround.stackables.compat.modmenu.ModMenuImpl"
    ]
  },
  "mixins": [
    "stackables.mixins.json"
  ],
  "accessWidener" : "stackables.accesswidener",
  "depends": {
    "fabricloader": ">=0.13.3",
    "minecraft": "1.19.x",
    "java": ">=17",
    "roundalib": "~0.2.0"
